{
  "env": {
    "commonjs": {
      "presets": [
        "@babel/preset-env"
      ],
      "plugins": [
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-proposal-class-properties"],
      "ignore": [
        "**/*.test.js",
        "**/__mocks__/**"
      ]
    },
    "test": {
      "presets": ["@babel/preset-env"],
      "plugins": [
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/plugin-proposal-class-properties"]
    }
  }
}
